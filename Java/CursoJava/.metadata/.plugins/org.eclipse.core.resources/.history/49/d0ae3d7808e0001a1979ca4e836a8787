package fundamentos;

public class inferencia20 {
	public static void main(String[] args) {
		double a = 4.5;
		System.out.println(a);
		
		a = 12;
		System.out.println(a);
		
		// colocando var, o java vai inferir o tipo da vari√°vel
		// nesse caso, double
		var b = 4.5;
		System.out.println(b);
		
		var c = 10;
		c = 9.6;
		System.out.println(c);
				
	}

}
