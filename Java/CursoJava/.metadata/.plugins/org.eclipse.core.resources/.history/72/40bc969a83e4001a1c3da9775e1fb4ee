package classeMetodo;

public class Data92 {
	
	String dataFormatada(String dia, String mes, String ano) {
		return dia+"/"+mes+"/"+ano;
	}
	String dataFormatada() {
		return String.format("olá mundo!!");
		// esse método é mais interessante do que usar 
		// ao retornar a string é possível
	}
	
	
}